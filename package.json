		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"optimize-js-plugin": "0.0.4",
		"style-loader": "^0.23.1",
		"ts-loader": "^4.5.0",
		"typedoc": "^0.14.2",
		"typescript": "^3.4.5",
		"uglifyjs-webpack-plugin": "^1.3.0",
		"webpack": "^4.30.0",
